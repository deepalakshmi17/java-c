import java.util.*;
class Oddoreven
{
public static void main(String arg[])
{
Scanner sc=new Scanner(System.in);
int a;
System.out.println("enter the a value");
a=sc.nextInt();
if(a%2==0)
{
System.out.println("the given number is even");
}
else 
{
System.out.println("the given number is odd");
}
}
}
