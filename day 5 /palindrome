class Palindrome
{
  public static void main(String arg[])
  {
    int num=121,rev=0,rem=0,temp=num;
    while(num!=0)
    { 
      rem=num%10;
      rev=rev*10+rem;
      num=num/10;
    }
    if(temp==rev){
      System.out.println("the given number is a palindrome");
     }
    else{
      System.out.println("the given number is not a palindrome");
     }
   }
}
