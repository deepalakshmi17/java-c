import java.util.*;
class Starsquarenumber
{
  public static void main(String arg[])
  {
    Scanner sc=new Scanner(System.in);
    int n,i,j,k=1;
    System.out.println("enter n");
    n=sc.nextInt();
    for(i=1;i<n;i++)
    {
      for(j=1;j<=i;j++)
      {
        System.out.print((k*k)+" ");
        k++;
      }
      System.out.println();
    }
  }
}
