import java.util.*;
class Display2
{
  public static void main(String arg[])
  {
    Scanner sc=new Scanner(System.in);
    int n;
    System.out.println("enter the number from which you want to display");
    n=sc.nextInt();
    while(n<10)
    { 
      System.out.println(n);
      n++;
    }
  }
}
